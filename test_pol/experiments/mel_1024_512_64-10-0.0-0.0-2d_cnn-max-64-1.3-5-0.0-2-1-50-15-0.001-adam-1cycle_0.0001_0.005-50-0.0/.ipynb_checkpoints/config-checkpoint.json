{
    "data": {
        "_input_dim": 64,
        "_kfold_seed": 42,
        "_n_folds": 5,
        "_train_data_dir": "data/Training_Data/",
        "_train_df": "data/train_df.csv",
        "features": "mel_1024_512_64",
