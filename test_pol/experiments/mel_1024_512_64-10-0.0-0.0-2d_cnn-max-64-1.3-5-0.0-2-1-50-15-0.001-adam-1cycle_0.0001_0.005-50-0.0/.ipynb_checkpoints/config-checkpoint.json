        "accumulation_steps": 1,
        "batch_size": 50,
        "epochs": 15,
        "learning_rate": 0.001,
        "optimizer": "adam",
        "scheduler": "1cycle_0.0001_0.005",
        "switch_off_augmentations_on": 50,
        "weight_decay": 0.0
